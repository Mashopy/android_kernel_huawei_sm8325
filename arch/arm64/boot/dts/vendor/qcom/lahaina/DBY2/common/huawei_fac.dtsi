&soc {
	ant_cable_det: ant_cable_det {
                compatible = "huawei,antenna_cable_detect";
                pinctrl-names = "default", "idle";
                pinctrl-0 = <&ant_cable_det_id0_default &ant_cable_det_id1_default &ant_cable_det_id2_default &ant_cable_det_id3_default &ant_cable_det_id4_default>;
                pinctrl-1 = <&ant_cable_det_id0_idle &ant_cable_det_id1_idle &ant_cable_det_id2_idle &ant_cable_det_id3_idle &ant_cable_det_id4_idle>;
                gpio_count = <5>;
                gpios = <&tlmm 123 0>,
                        <&tlmm 42 0>,
						<&tlmm 33 0>,
                        <&tlmm 62 0>,
						<&tlmm 63 0>;
                debounce-interval = <15>;
	};
};
