&dsi_dby2_290_20d_11p5_qcom {
        qcom,mdss-dsi-pan-enable-dynamic-fps;
        qcom,enable-supermotion;
        qcom,dsi-supported-dfps-list = <144 120 60 30>;
        qcom,dsi-supported-qsync-min-fps-list = <120 113 50 30>;
        qcom,mdss-dsi-pan-fps-update = "dfps_immediate_porch_mode_vfp";
        qcom,mdss-dsi-min-refresh-rate = <30>;
        qcom,mdss-dsi-max-refresh-rate = <144>;
        qcom,mdss-dsi-display-timings {
                timing@0 {
