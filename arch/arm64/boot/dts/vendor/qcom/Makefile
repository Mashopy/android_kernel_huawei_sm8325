ifeq ($(CONFIG_ARCH_LAHAINA),y)
ifeq ($(CONFIG_BUILD_ARM64_DT_OVERLAY),y)
HUAWEI_PLATFORM_DIR := lahaina
HUAWEI_JAD_DIR := $(HUAWEI_PLATFORM_DIR)/JAD
HUAWEI_JAD_PROJECTS := $(HUAWEI_JAD_DIR)/AL00_VA $(HUAWEI_JAD_DIR)/AL00_VM $(HUAWEI_JAD_DIR)/AL00_VL $(HUAWEI_JAD_DIR)/AL00_VG $(HUAWEI_JAD_DIR)/LX9_VE $(HUAWEI_JAD_DIR)/AL00_VD $(HUAWEI_JAD_DIR)/AL00_VH $(HUAWEI_JAD_DIR)/AL00_VF $(HUAWEI_JAD_DIR)/LX9_VC $(HUAWEI_JAD_DIR)/AL00_VI $(HUAWEI_JAD_DIR)/AL00_VE $(HUAWEI_JAD_DIR)/AL00_VC $(HUAWEI_JAD_DIR)/AL00_VK $(HUAWEI_JAD_DIR)/LX9_VA $(HUAWEI_JAD_DIR)/LX9_VB $(HUAWEI_JAD_DIR)/AL00_VJ $(HUAWEI_JAD_DIR)/AL00_VB $(HUAWEI_JAD_DIR)/LX9_VD
HUAWEI_TARGET_PROJECTS := $(HUAWEI_JAD_PROJECTS)
endif

LAHAINA_BASE_DTB += lahaina.dtb lahaina-v2.dtb lahaina-v2.1.dtb
dtb-y += $(LAHAINA_BASE_DTB)
endif

always		:= $(dtb-y)
subdir-y	:= $(HUAWEI_TARGET_PROJECTS)
clean-files	:= *.dtb
