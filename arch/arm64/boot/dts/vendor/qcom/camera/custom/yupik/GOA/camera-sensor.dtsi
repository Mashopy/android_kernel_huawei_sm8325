		eeprom-src = <&eeprom_front_tel>;
		cam_vana-supply = <&xpmic_ldo3>; /* mcam avdd 2.85v */
		cam_vio-supply = <&xpmic_ldo5>; /* mcam iovdd 1.8v */
		cam_vdig-supply = <&xpmic_ldo1>; /* mcam dvdd 1.14v */
		cam_vaf-supply = <&xpmic_ldo4>; /* afvdd pm8350c l11 3.1v */
		cam_clk-supply = <&cam_cc_titan_top_gdsc>;
		regulator-names = "cam_vana", "cam_vio", "cam_vdig","cam_vaf", "cam_clk";
		rgltr-cntrl-support;
		rgltr-min-voltage = <2850000 1800000 1140000 3100000 0>;
		rgltr-max-voltage = <2850000 1800000 1140000 3100000 0>;
		rgltr-load-current = <4000 4000 4000 0 0>; /* value? */
