<hwlcd>
	<Version>1.1</Version>

	<!-- any supported lcd Panel, add it here! very important -->
	<lcdlist>
		<!-- format: -->
		<!-- panel-name, gpio, brdid(product_id), product_name, product_floder, version_floder -->
		<!-- panel-name : must same with panel xml file name -->
		<!-- product & version folder: must same with the folder name or 'def' -->
		<lcd>raydium-6d030-1p41-cmd,                    0x00,   1000,    arc,    arc,  def     </lcd>
	</lcdlist>

	<PanelEntry>
		<!--Panel type:dsi_video_mode-video mode, dsi_cmd_mode-command mode-->
		<PanelTypeMode>"dsi_video_mode"</PanelTypeMode>
		<PanelModeSwitch>0</PanelModeSwitch>
		<PanelPhyWidth>69</PanelPhyWidth>
		<PanelPhyHeight>154</PanelPhyHeight>

		<!--Dsi ctrl number:1-one dsi ctrl, 2-two dsi ctrl-->
		<DsiCtrlNum>0</DsiCtrlNum>
		<!--Dsi phy number:1-one dsi phy, 2-two dsi phy-->
		<DsiPhyNum>0</DsiPhyNum>
		<PanelSupplyEntries>"dsi_panel_pwr_supply"</PanelSupplyEntries>

		<!-- Panel Color Information -->
		<PanelColorFormat>24</PanelColorFormat>
		<PanelColorOrder>"rgb_swap_rgb"</PanelColorOrder>
		<PanelUnderFlowColor>0xff</PanelUnderFlowColor>
		<PanelBorderColor>0</PanelBorderColor>

		<!-- DSI Video Config -->
		<PanelVirChannelId>0</PanelVirChannelId>
		<DMASchedLine>1</DMASchedLine>
		<PanelTrafficMode>"burst_mode"</PanelTrafficMode>
		<PanelHSyncPulse>0</PanelHSyncPulse>
		<PanelHFPPowerMode>0</PanelHFPPowerMode>
		<PanelHBPPowerMode>0</PanelHBPPowerMode>
		<PanelHSAPowerMode>0</PanelHSAPowerMode>
		<PanelLastLineInterleave>0</PanelLastLineInterleave>
		<PanelBLLPEOFPowerMode>1</PanelBLLPEOFPowerMode>
		<PanelBLLPPowerMode>1</PanelBLLPPowerMode>

		<!-- Lane Config -->
		<PanelDSILaneMap>"lane_map_0123"</PanelDSILaneMap>
		<PanelLane0State>1</PanelLane0State>
		<PanelLane1State>1</PanelLane1State>
		<PanelLane2State>1</PanelLane2State>
		<PanelLane3State>1</PanelLane3State>

		<!-- Trigger Config -->
		<!--none, trigger_te, trigger_sw, trigger_sw_te-->
		<PanelDsiMDPTrigger>"none"</PanelDsiMDPTrigger>
		<!--none, trigger_te, trigger_sw, trigger_sw_te, trigger_sw_seof-->
		<PanelDsiDMATrigger>"trigger_sw"</PanelDsiDMATrigger>
		<PanelTEPinSelect>0</PanelTEPinSelect>

		<!-- MIPI Host Config -->
		<DsiTClkPost>0</DsiTClkPost>
		<DsiTClkPre>0</DsiTClkPre>
		<DsiRxEotIgnore>0</DsiRxEotIgnore>
		<DsitxEotAppend>0</DsitxEotAppend>
		<DsiExtBridgeMode>0</DsiExtBridgeMode>
		<DsiForceClockLaneHs>0</DsiForceClockLaneHs>
		<DsiSelectClocks>"mux_byte_clk0", "mux_pixel_clk0", "src_byte_clk0", "src_pixel_clk0", "shadow_byte_clk0", "shadow_pixel_clk0"</DsiSelectClocks>
		<!-- 0:DPHY  1:CPHY -->
		<DsiPanelCphyMode>0</DsiPanelCphyMode>
		<DsiUlpsEnabled>0</DsiUlpsEnabled>
		<DsiSuspendUlpsEnabled>0</DsiSuspendUlpsEnabled>
		<DSIStream>0</DSIStream>
		<DsiTeUsingWd>0</DsiTeUsingWd>
		<DsiCmdSyncWaitBroadcast>0</DsiCmdSyncWaitBroadcast>
		<DsiLp11Init>1</DsiLp11Init>
		<!-- if ThreeStagePowerOn is set,poweron sequence: PowerOnStage LPOnStage HSOnStage-->
		<!-- if not set,poweron sequence: PowerOnStage LPOnStage-->
		<ThreeStagePowerOn>0</ThreeStagePowerOn>
		<!-- AOD LP1-LP2-LP1 ,if ApAodLp2NeedSendCmd is set,LP2 to LP1 need send DSI_CMD_SET_LP1 cmd -->
		<!-- if ApAodLp2NeedSendCmd not set,LP2 to LP1 can not send DSI_CMD_SET_LP1 cmd -->
		<ApAodLp2NeedSendCmd>1</ApAodLp2NeedSendCmd>

		<!-- Split Link Config -->
		<DsiSplitLinkEnable>0</DsiSplitLinkEnable>
		<DsiSublinksCount>2</DsiSublinksCount>
		<DsiLanesPerSublink>2</DsiLanesPerSublink>

		<!-- HDR Config -->
		<DsiPanelHdrEnabled>0</DsiPanelHdrEnabled>
		<HdrColorPrimaries>14500 15500 32000 17000 15500 30000 8000 3000</HdrColorPrimaries>
		<DsiPanelPeakBrightness>4200000</DsiPanelPeakBrightness>
		<DsiPanelBlacknessLevel>3230</DsiPanelBlacknessLevel>

		<!-- DMS Config -->
		<DsiPanelDynamicModeSwitchEnabled>0</DsiPanelDynamicModeSwitchEnabled>
		<DsiPanelDynamicModeSwitchType>"dynamic-resolution-switch-immediate"</DsiPanelDynamicModeSwitchType>

		<!--Esd enable-->
		<PanelEsdSupport>1</PanelEsdSupport>
		<!--Set Bl After First Frame Done-->
		<PanelBlAfterFrame>0</PanelBlAfterFrame>
		<!--Project id support-->
		<PanelProjectIdSupport>0</PanelProjectIdSupport>
		<!--Tp Project Id-->
		<PanelDefaultTpProjectId>"G21579250"</PanelDefaultTpProjectId>

		<!--gpio offset number setting-->
		<PanelGpioOffset>308</PanelGpioOffset>
		<!--finger unlock state support-->
		<PanelFingerUnlockStateSupport>0</PanelFingerUnlockStateSupport>
		<!--bl max type:0-not support, 1-ddic ytpe-->
		<PanelGetBlMaxNitType>0</PanelGetBlMaxNitType>
		<PanelGetBlMaxNitOffset>400</PanelGetBlMaxNitOffset>
		<!--bl max nit-->
		<PanelBlMaxnit>450</PanelBlMaxnit>
		<!--bl min brightness setting-->
		<PanelBlmin>4</PanelBlmin>
		<!--bl max brightness setting-->
		<PanelBlmax>255</PanelBlmax>
		<!--bl def brightness setting-->
		<PanelBldef>128</PanelBldef>
		<!--bl ctrl type setting-->
		<PanelBlIcCtrlType>0</PanelBlIcCtrlType>
		<!--bias ctrl type setting-->
		<PanelBiasIcCtrlType>0</PanelBiasIcCtrlType>
		<!--Running test config start-->
		<!--Dot/column inversion test support-->
		<PanelInversionSupport>0</PanelInversionSupport>
		<!--Check panel power status test support-->
		<PanelCheckRegSupport>0</PanelCheckRegSupport>
		<!--Pt station test support-->
		<PanelPtTestSupport>0</PanelPtTestSupport>
		<!--sn code support-->
		<PanelSnCodeSupport>0</PanelSnCodeSupport>
		<!--Thp Proximity support-->
		<PanelThpProximitySupport>0</PanelThpProximitySupport>
		<ThpProximityResetDelayMin>65</ThpProximityResetDelayMin>
		<!--PanelOnAlwaysNeedReset-->
		<PanelOnAlwaysNeedReset>0</PanelOnAlwaysNeedReset>
		<!-- Backlight ctrl config -->
		<BlPmicControlType>"bl_ctrl_external"</BlPmicControlType>
		<!--backlight order-->
		<BacklightOrder>0</BacklightOrder>
		<BacklightNeedSync>0</BacklightNeedSync>

		<!-- Display dsi timming config -->
		<DisplayDsiTimmingBegin>1</DisplayDsiTimmingBegin>
		<DisplayDsiTimming0Begin>1</DisplayDsiTimming0Begin>
		<DisplayDsiTimming0End>1</DisplayDsiTimming0End>
		<DisplayDsiTimming1Begin>0</DisplayDsiTimming1Begin>
		<DisplayDsiTimming1End>0</DisplayDsiTimming1End>
		<DisplayDsiTimming2Begin>0</DisplayDsiTimming2Begin>
		<DisplayDsiTimming2End>0</DisplayDsiTimming2End>
		<DisplayDsiTimmingEnd>1</DisplayDsiTimmingEnd>

		<!-- Display dsi timming0 config -->
		<PanelXres0>466</PanelXres0>
		<PanelYres0>466</PanelYres0>
		<HFrontPorch0>10</HFrontPorch0>
		<HBackPorch0>10</HBackPorch0>
		<HPulseWidth0>2</HPulseWidth0>
		<VFrontPorch0>10</VFrontPorch0>
		<VBackPorch0>10</VBackPorch0>
		<VPulseWidth0>2</VPulseWidth0>
		<HSyncSkew0>0</HSyncSkew0>
		<PanelFrameRate0>60</PanelFrameRate0>
		<PanelPhyTimings0>"00 0C 03 03 10 1D 03
				03 03 02 02 04 0B 08"</PanelPhyTimings0>
		<DisplayTopology0>1 0 1</DisplayTopology0>
		<DefaultTopologyIndex0>0</DefaultTopologyIndex0>

		<!--Check panel power status test support-->
		<PanelCheckRegSupport>0</PanelCheckRegSupport>
		<PanelCheckRegOnSupport>0</PanelCheckRegOnSupport>
		<PanelCheckRegOnSupportDsmReport>0</PanelCheckRegOnSupportDsmReport>
		<PanelCheckAodRegOnSupport>0</PanelCheckAodRegOnSupport>
		<PanelCheckAodRegOnSupportDsmReport>0</PanelCheckAodRegOnSupportDsmReport>
		<PanelCheckRegOffSupport>0</PanelCheckRegOffSupport>
		<PanelCheckRegOffSupportDsmReport>0</PanelCheckRegOffSupportDsmReport>
		<!-- for mipi check reg on -->
		<PanelCheckRegOnCommand>"0x06, 0x01, 0x00, 0x00, 0x0a, 0x00, 0x01, 0x0a"</PanelCheckRegOnCommand>
		<PanelCheckRegOnValue>"0x9c"</PanelCheckRegOnValue>
		<PanelCheckRegOnCommandState>0</PanelCheckRegOnCommandState>
		<!-- for mipi check aod on -->
		<PanelCheckAodRegOnCommand>"0x06, 0x01, 0x00, 0x00, 0x0a, 0x00, 0x01, 0x0a"</PanelCheckAodRegOnCommand>
		<PanelCheckAodRegOnValue>"0xdc"</PanelCheckAodRegOnValue>
		<PanelCheckAodRegOnCommandState>0</PanelCheckAodRegOnCommandState>
		<!-- for mipi check reg off -->
		<PanelCheckRegOffCommand>"0x06, 0x01, 0x00, 0x00, 0x0a, 0x00, 0x01, 0x0a"</PanelCheckRegOffCommand>
		<PanelCheckRegOffValue>"0x08"</PanelCheckRegOffValue>
		<PanelCheckRegOffCommandState>0</PanelCheckRegOffCommandState>
		<!--event send
		event, data, delay/sync, no-sync
		event:0-NONE, 1-VCI, 2-IOVCC, 3-VSP, 4-VSN, 5-RESET, 6-MIPI init code, 7-TS early-event, 8-TS later-event, 9-VDD, 10-AOD, 11-BIAS
		data:0-power off, 1-power on
		delay:ms/TS:0-no-sync 1-sync
		-->
		<!--Power on timming-->
		<PowerOnStage>"3, 1, 0
				4, 1, 0
				11, 1, 0"</PowerOnStage>

		<!--Power off timming-->
		<PowerOffStage>"4, 0, 0
				3, 0, 0"</PowerOffStage>

	</PanelEntry>
</hwlcd>
